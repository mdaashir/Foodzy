		}
	],
	"require": {
		"ext-pgsql": "*",
		"vlucas/phpdotenv": "^5.6"
	},
	"scripts": {
		"start": "php -S localhost:8000"
	},
	"watch": {
		"composer_json": {
			"files": [
				"composer.json",
				"composer.lock"
			],
			"tasks": [
				"exec:composer_install"
			]
		}
	},
	"exec": {
		"composer_install": {
			"cmd": "composer self-update && composer install --no-dev",
			"exitCode": [
				0,
				255
			]
		}
	}
}
